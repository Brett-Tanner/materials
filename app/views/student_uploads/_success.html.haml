-# locals: (student:)

%tr.uploaded{ data: { controller: 'upload-summary',
                      'upload-summary-status-value': 'Uploaded' } }
  %td.bg-success= student.name
  %td.bg-success= student.student_id
  %td.bg-success= student.level
  %td.bg-success= student.school.name
  %td.bg-success= student.parent_id
  %td.bg-success= student.start_date
  %td.bg-success= student.quit_date
  %td.bg-success= student.birthday
  %td.bg-success.flex.items-center.justify-center.gap-2.p-2
    %p Uploaded
    %span.material-symbols-outlined download_done
